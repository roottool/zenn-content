{
	"name": "zenn-content",
	"version": "1.0.0",
	"description": "My content of Zenn",
	"private": true,
	"author": "roottool",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/roottool/zenn-content.git"
	},
	"bugs": {
		"url": "https://github.com/roottool/zenn-content/issues"
	},
	"homepage": "https://github.com/roottool/zenn-content#readme",
	"scripts": {
		"preview": "zenn preview",
		"new:article": "zenn new:article",
		"new:book": "zenn new:book",
		"check:markdownlint": "markdownlint-cli2 README.md \".github/**/*.md\" \"articles/**/*.md\" \"books/**/*.md\"",
		"check:textlint": "textlint --cache \"**/*.md\"",
		"check:prettier": "prettier --cache --check \"**/*.{md,json,jsonc,yml,yaml}\"",
		"check": "npm-run-all2 check:markdownlint check:textlint check:prettier",
		"check:textlint:ci": "textlint \"**/*.md\"",
		"check:prettier:ci": "prettier --check \"**/*.{json,jsonc,yml,yaml}\"",
		"check:ci": "npm-run-all2 check:markdownlint check:textlint:ci check:prettier:ci",
		"fix:markdownlint": "markdownlint-cli2 README.md \".github/**/*.md\" \"articles/**/*.md\" \"books/**/*.md\" --fix",
		"fix:textlint": "textlint \"**/*.md\" --fix",
		"fix:prettier": "prettier --cache --write \"**/*.{md,json,jsonc,yml,yaml}\"",
		"fix": "npm-run-all2 fix:markdownlint fix:textlint fix:prettier"
	},
	"devDependencies": {
		"@textlint-ja/textlint-rule-preset-ai-writing": "1.6.1",
		"markdownlint-cli2": "0.20.0",
		"npm-run-all2": "8.0.4",
		"prettier": "3.7.4",
		"textlint": "15.5.0",
		"textlint-rule-ja-unnatural-alphabet": "2.0.1",
		"textlint-rule-max-ten": "5.0.0",
		"textlint-rule-no-doubled-joshi": "5.1.1",
		"textlint-rule-preset-icsmedia": "github:ics-creative/textlint-rule-preset-icsmedia",
		"textlint-rule-preset-ja-technical-writing": "12.0.2",
		"textlint-rule-preset-jtf-style": "3.0.3",
		"textlint-rule-prh": "6.1.0",
		"zenn-cli": "0.2.10"
	},
	"license": "MIT"
}
